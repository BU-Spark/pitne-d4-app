{
    "compilerOptions": {
      "target": "es5",
      "module": "esnext",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noImplicitAny": false,
      "moduleResolution": "node"  // Explicitly set moduleResolution to 'node'
    },
    "include": ["src/**/*"]
  }
  